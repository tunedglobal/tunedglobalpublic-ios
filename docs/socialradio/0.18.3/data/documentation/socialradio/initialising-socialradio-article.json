odeListing","syntax":"swift"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"text":"SocialRadio requires a unique device identifier, generally the identifierForVendor provided by Apple should work: Provided via","type":"text"},{"text":" ","type":"text"},{"identifier":"doc:\/\/org.cocoapods.SocialRadio\/documentation\/SocialRadio\/TunedRestDelegate\/uniqueDeviceIdentifier()","type":"reference","isActive":true}]}]}],"type":"unorderedList"},{"code":["public class MyTunedRestDelegate: NSObject, TunedRestDelegate {","    \/\/ ...","    public func uniqueDeviceIdentifier() -> Void) -> String {","        \/\/ ...","        let myUniqueIdentifier = String(identifierForVendor)","        return myUniqueIdentifier","    }","    \/\/ ...","}"],"type":"codeListing","syntax":"swift"},{"level":4,"text":"SocialRadioUIDelegate","type":"heading","anchor":"SocialRadioUIDelegate"},{"inlineContent":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.cocoapods.SocialRadio\/documentation\/SocialRadio\/SocialRadioUIDelegate"},{"type":"text","text":" is responsible for providing data required for the Radio UI to function, as well as presenting UI SocialRadio is not able to do on its own."}],"type":"paragraph"},{"items":[{"content":[{"inlineContent":[{"text":"SocialRadio allows the user to add one or multiple tracks to a radio show. When that action is triggered, SocialRadio asks this delegate to provide the UI for selecting them. This is done via ","type":"text"},{"identifier":"doc:\/\/org.cocoapods.SocialRadio\/documentation\/SocialRadio\/SocialRadioUIDelegate\/presentTracklist(filterOutExplicitContent:completion:)","isActive":true,"type":"reference"}],"type":"paragraph"}]}],"type":"unorderedList"},{"code":["public class MySocialRadioUIDelegate: NSObject, SocialRadioUIDelegate {","    \/\/ ...","    func presentTracklist(filterOutExplicitContent: Bool,","                          completion: @escaping (_ tracks: [TunedTrackModel]?,"," 